import udif

content = '''
addresses:
  -
    name: dave
    addr:
      > 123 Surf Street
      > Santa Cruz, CA
  -
    name: claudia
    addr:
      > 456 Ocean Ave
      > Monterrey, CA
errors:
  
  	   > hello
'''
try:
    data = udif.loads(content)
    print(udif.dumps(data))
except udif.Error as e:
    e.report()
